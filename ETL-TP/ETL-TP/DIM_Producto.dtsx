                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    P.id_producto AS producto_key,
    CProd.descripcion AS nombre,          -- descripción del producto
    CFam.descripcion AS categoria,        -- familia como categoría
    CDur.descripcion AS duracion          -- duración
FROM Productos P
JOIN Codigos CProd ON P.id_producto = CProd.codigo AND CProd.tipo = 'producto'
JOIN Codigos CFam  ON P.id_familia  = CFam.codigo  AND CFam.tipo = 'familia'
JOIN Codigos CDur  ON P.id_duracion = CDur.codigo  AND CDur.tipo = 'duracion';</property>
